{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Dev Script",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
    //   "port": 9229,
    //   "console": "integratedTerminal", terminal copies the debug console
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/backend"
    }
  ]
}
